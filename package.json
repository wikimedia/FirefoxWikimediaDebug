{
    "name": "wikimediadebug",
    "title": "Wikimedia Debug Header",
    "id": "wikimediadebug@wikimedia.org",
    "description": "Inject 'X-Wikimedia-Debug' header into HTTP requests.",
    "author": "Bryan Davis <bd808@wikimedia.org>",
    "contributors": [
        "Ori Livneh <ori@wikimedia.org>"
    ],
    "icon": "data/icon.png",
    "icon64": "data/icon_64.png",
    "license": "Apache-2.0",
    "main": "lib/main.js",
    "permissions": {
        "private-browsing": true
    },
    "preferences": [{
        "name": "backend",
        "title": "Backend server",
        "description": "Backend server requests should be sent to",
        "type": "string",
        "value": "mw1017.eqiad.wmnet"
    },
    {
        "name": "timeout",
        "title": "Timeout",
        "description": "Disable debug header after this many minutes",
        "type": "integer",
        "value": 15
    }],
    "version": "0.5.0"
}
